<div class="container-fluid login-form">
  <%= form_for :session, url: login_path do |f|%>
    <%= f.text_field :username, placeholder: "Username" %>
    <%= f.password_field :password, placeholder: "Password" %>
    <%= f.submit "Login" %>
  <% end %>
    <hr>
  <%= link_to 'Create Account', new_user_path %>
</div>
